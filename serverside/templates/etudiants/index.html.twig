{% extends 'base.html.twig' %}

{% block title %}Etudiants index{% endblock %}

{% block body %}
    <h1>Etudiants index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Nom_etudiants</th>
                <th>Password</th>
                <th>Prenom_etudiants</th>
                <th>Email</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for etudiant in etudiants %}
            <tr>
                <td>{{ etudiant.id }}</td>
                <td>{{ etudiant.nomEtudiants }}</td>
                <td>{{ etudiant.password }}</td>
                <td>{{ etudiant.prenomEtudiants }}</td>
                <td>{{ etudiant.email }}</td>
                <td>
                    <a href="{{ path('app_etudiants_show', {'id': etudiant.id}) }}">show</a>
                    <a href="{{ path('app_etudiants_edit', {'id': etudiant.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="6">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_etudiants_new') }}">Create new</a>
{% endblock %}
